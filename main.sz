FUNKCYJO kwadrat(x)
    NAZOT x * x
KONIEC

FUNKCYJO print_list(numery)
    DLA i = LEN(numery) - 1 DO 0 CO -1
        GODEJ numery[i]
    NEXT
KONIEC

ZMIYNNO p = -2
ZMIYNNO q = 2

JAK p < 0
POTYM
    ZMIYNNO wynik = kwadrat(-2)
    GODEJ wynik
INKSZY JAK p > 2
POTYM
    ZMIYNNO wynik = kwadrat(6)
    GODEJ wynik
KONIEC

ZMIYNNO numery = [1, 2, 3]
print_list(numery)


