	10 [label=5]
	9 -> 10
	11 [label="
"]
	1 -> 11
	12 [label=statement]
	1 -> 12
	13 [label=assignmentstmt]
	12 -> 13
	14 [label=ZMIYNNO]
	13 -> 14
