	1 -> 40
	41 [label=statement]
	1 -> 41
	42 [label=printstmt]
	41 -> 42
	43 [label=GODEJ]
	42 -> 43
	44 [label=expression]
	42 -> 44
	45 [label=id]
	44 -> 45
	46 [label=suma]
	45 -> 46
	47 [label="
"]
	1 -> 47
	48 [label=<EOF>]
