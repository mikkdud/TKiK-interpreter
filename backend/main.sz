FUNKCYJO kwadrat(x)
    NAZOT x * x
KONIEC

FUNKCYJO print_list(numery)
    DLA i = LEN(numery) - 1 DO 0 CO -1
        GODEJ numery[i]
    NEXT
KONIEC

